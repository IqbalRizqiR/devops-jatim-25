{
    "family": "lks-leantime-app",
    "containerDefinitions": [
        {
            "name": "lks-leantime",
            "image": "leantime/leantime:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/lks/leantime-app",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "cpu": 1024,
            "memory": 2048,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "name": "lks-leantime-port"
                }
            ],
            "environment": [
                {
                    "name": "LEAN_APP_URL",
                    "value": "http://lks-oss-lb-645559201.us-east-1.elb.amazonaws.com"
                },
                {
                    "name": "LEAN_APP_DIR",
                    "value": "/"
                },
                {
                    "name": "LEAN_DEBUG",
                    "value": 0
                },
                {
                    "name": "LEAN_DEFAULT_TIMEZONE",
                    "value": "Asia/Jakarta"
                },
                {
                    "name": "PGID",
                    "value": 1000
                },
                {
                    "name": "PUID",
                    "value": 1000
                }
            ],
            "secrets": [
                { "name": "LEAN_DB_HOST", "valueFrom": "arn:aws:ssm:us-east-1:322716019423:parameter/lks/db/host" },
                { "name": "LEAN_DB_USER", "valueFrom": "arn:aws:ssm:us-east-1:322716019423:parameter/lks/db/username" },
                { "name": "LEAN_DB_PASSWORD", "valueFrom": "arn:aws:ssm:us-east-1:322716019423:parameter/lks/db/password" },
                { "name": "LEAN_DB_DATABASE", "valueFrom": "arn:aws:ssm:us-east-1:322716019423:parameter/lks/db/name" },
                { "name": "LEAN_DB_PORT", "valueFrom": "arn:aws:ssm:us-east-1:322716019423:parameter/lks/db/port" }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "lks-volume-public_userfiles",
                    "containerPath": "/var/www/html/public/userfiles"
                },
                {
                    "sourceVolume": "lks-volume-userfiles",
                    "containerPath": "/var/www/html/userfiles"
                },
                {
                    "sourceVolume": "lks-volume-plugins",
                    "containerPath": "/var/www/html/app/Plugins"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::322716019423:role/LabRole",
    "executionRoleArn": "arn:aws:iam::322716019423:role/LabRole",
    "networkMode": "awsvpc",
    "awsVpcConfiguration": {
        "subnets": ["subnet-045c9fcf9bcf2b9ae", "subnet-0a4eb6e6149266778 "],
        "securityGroups": ["sg-0d1d67182684a131d"],
        "assignPublicIp": "DISABLED"
    },
    "volumes": [
        {
            "name": "lks-volume-public_userfiles",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-0741f985362330fdb",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-090dac3d9aa26a510",
                    "iam": "ENABLED"
                }
            }
        },
        {
            "name": "lks-volume-userfiles",
            "efsVolumeConfiguration": {
                "fileSystemId": "fsap-0e54fc992c48fb6de",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-0e54fc992c48fb6de",
                    "iam": "ENABLED"
                }
            }
        },
        {
            "name": "lks-volume-plugins",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-0741f985362330fdb",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-03173d6e9205cf67e",
                    "iam": "ENABLED"
                }
            }
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}