{
    "family": "lks-leantime-app",
    "containerDefinitions": [
        {
            "name": "lks-leantime",
            "image": "leantime/leantime:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/lks/leantime-app",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "cpu": 1024,
            "memory": 2048,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "name": "lks-leantime-port"
                }
            ],
            "environment": [
                {
                    "name": "LEAN_APP_URL",
                    "value": "http://lks-oss-lb-1427542545.us-east-1.elb.amazonaws.com"
                },
                {
                    "name": "LEAN_APP_PORT",
                    "value": "8080"
                },
                {
                    "name": "LEAN_APP_DIR",
                    "value": "/"
                },
                {
                    "name": "LEAN_DEBUG",
                    "value": "0"
                },
                {
                    "name": "LEAN_SITENAME",
                    "value": "LKS Leantime"
                },
                {
                    "name": "LEAN_DEFAULT_TIMEZONE",
                    "value": "Asia/Jakarta"
                },
                {
                    "name": "PGID",
                    "value": "1000"
                },
                {
                    "name": "PUID",
                    "value": "1000"
                }
            ],
            "secrets": [
                { "name": "LEAN_DB_HOST", "valueFrom": "arn:aws:ssm:us-east-1:746761418387:parameter/lks/db/host" },
                { "name": "LEAN_DB_USER", "valueFrom": "arn:aws:ssm:us-east-1:746761418387:parameter/lks/db/username" },
                { "name": "LEAN_DB_PASSWORD", "valueFrom": "arn:aws:ssm:us-east-1:746761418387:parameter/lks/db/password" },
                { "name": "LEAN_DB_DATABASE", "valueFrom": "arn:aws:ssm:us-east-1:746761418387:parameter/lks/db/name" },
                { "name": "LEAN_DB_PORT", "valueFrom": "arn:aws:ssm:us-east-1:746761418387:parameter/lks/db/port" }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "lks-volume-public_userfiles",
                    "containerPath": "/var/www/html/public/userfiles"
                },
                {
                    "sourceVolume": "lks-volume-userfiles",
                    "containerPath": "/var/www/html/userfiles"
                },
                {
                    "sourceVolume": "lks-volume-plugins",
                    "containerPath": "/var/www/html/app/Plugins"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::746761418387:role/LabRole",
    "executionRoleArn": "arn:aws:iam::746761418387:role/LabRole",
    "networkMode": "awsvpc",
    "awsVpcConfiguration": {
        "subnets": ["subnet-0c1e0d0e9e3801045", "subnet-0d8c7c7130448aa0c "],
        "securityGroups": ["sg-00cd7515f8154631a"],
        "assignPublicIp": "DISABLED"
    },
    "volumes": [
        {
            "name": "lks-volume-public_userfiles",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-0303664d8251860b5",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-0788eb1b5dd4c2bb4",
                    "iam": "ENABLED"
                }
            }
        },
        {
            "name": "lks-volume-userfiles",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-0303664d8251860b5",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-0fce2a906223c784b",
                    "iam": "ENABLED"
                }
            }
        },
        {
            "name": "lks-volume-plugins",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-0303664d8251860b5",
                "rootDirectory": "/",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "accessPointId": "fsap-004067d18a2fff92c",
                    "iam": "ENABLED"
                }
            }
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}